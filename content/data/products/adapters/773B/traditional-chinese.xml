      <product:techspec id="2" name="供電量">
	  <textbox value="130" id="1"/>W
      </product:techspec>
	  <product:techspec id="3" name="隨附纜線">
      <textbox value="6" id="3"/><dropdownlist id="meter-feet-unit"/> 電源線
      </product:techspec>
    </product:techspecs>
    <!-- End of Name & Value Columns (PIM - PRC Upload Usage) -->

  </generic:content>
