      <product:techspec id="1" name="デバイスタイプ">
      ACアダプタ
      </product:techspec>
      <product:techspec id="2" name="提供電源">
	  <textbox value="130" id="1"/>W
      </product:techspec>
	  <product:techspec id="3" name="付属ケーブル">
	  <textbox value="6" id="3"/><dropdownlist refid="meter-feet-unit"/> 電源コード
      </product:techspec>
    </product:techspecs>
    <!-- End of Name & Value Columns (PIM - PRC Upload Usage) -->

  </generic:content>
